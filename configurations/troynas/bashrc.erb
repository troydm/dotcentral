# some useful aliases
alias temp="watch -n 1 'cat /tmp/tmux-info && echo'"

# Add bin directory to PATH
add_PATH "<%= pwd+"/bin" %>"
add_PATH /opt/swift/usr/bin
add_PATH ~/.roswell/bin

# Neomutt alias
alias mutt=neomutt

# asdf
. $HOME/.asdf/asdf.sh
. $HOME/.asdf/completions/asdf.bash
export ASDF_CONCURRENCY=4

# opam
eval `opam env`
