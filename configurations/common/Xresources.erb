<% if hostname == 'troymac' %>
Xft.dpi: 192
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
Xcursor*theme: Breeze
Xcursor*size: 40
URxvt*buffered: false
<% end %>
URxvt.font: xft:DejaVuSansMono Nerd Font Mono:size=10
XTerm*faceName: DejaVuSansMono Nerd Font Mono
XTerm*faceSize: 10
URxvt*scrollBar: false
URxvt*termName: screen-256color
URxvt*xftAntialias: true
URxvt*depth: 32
*background: #3f3f3f
*foreground: #dcdccc
*cursorColor: #aaaaaa
*colorUL: #cc9393
URxvt*underlineColor: #dfaf8f
*color0: #3f3f3f
*color1: #cc9393
*color2: #7f9f7f
*color3: #d0bf8f
*color4: #6ca0a3
*color5: #dc8cc3
*color6: #8cd0d3
*color7: #dcdccc
*color8: #000000
*color9: #dca3a3
*color10: #bfebbf
*color11: #f0dfaf
*color12: #8cd0d3
*color13: #ec93d3
*color14: #93e0e3
*color15: #ffffff
URxvt.perl-ext-common:  default,clipboard,fullscreen,url-select
URxvt.keysym.F11:       perl:fullscreen:switch
URxvt.iso14755: False
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.button: 1C
URxvt.url-select.launcher: firefox
URxvt.tabbed.tab-fg: 216
URxvt.tabbed.tab-bg: 0
URxvt*meta8: false
URxvt.keysym.C-Up: \033[1;5A
URxvt.keysym.C-Down: \033[1;5B
URxvt.keysym.C-Left: \033[1;5D
URxvt.keysym.C-Right: \033[1;5C
URxvt.keysym.C-S-Up: \033[1;6A
URxvt.keysym.C-S-Down: \033[1;6B
URxvt.keysym.C-S-Left: \033[1;6D
URxvt.keysym.C-S-Right: \033[1;6C
URxvt.keysym.M-Left: \033[1;3D
URxvt.keysym.M-Right: \033[1;3C
URxvt.keysym.M-Up: \033[1;3A
URxvt.keysym.M-Down: \033[1;3B

rofi.bg: #333333
rofi.bgalt: #222222
rofi.bw: 2
rofi.bc: #000000
rofi.hlbg: #000000
rofi.fg: #DDDDDD
rofi.opacity: 80

